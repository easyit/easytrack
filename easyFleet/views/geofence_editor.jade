extends layout

block scripts
	link(rel='stylesheet', href='/stylesheets/map_drawing_tools.css')
	script(src='/lib/jquery.form.js')
	script(src='/js/views/geofence/geofence_editor.js')
	script(src='/js/controllers/logoutController.js')

	script(type="text/javascript", src="http://maps.google.com/maps/api/js?sensor=false&libraries=drawing")
	script(type="text/javascript", src="/lib/jquery-ui-map/jquery.ui.map.min.js")
 


block content
	include menu
	- user = typeof(udata) != 'undefined' ? udata : {}
	div.container-fluid
		div.row-fluid
			div.span4
				//- form#update-geofenceform(action="/geofence", method="POST")
				div#mainContent_warning
				include geofence_sidebar
				
			div.span8
				div.rounded.item.dark
					div#map_canvas(style="height: 400px;")


	include modals/alert
	include modals/confirm
		
